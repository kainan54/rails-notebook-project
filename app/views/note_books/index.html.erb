<p id="notice"><%= notice %></p>

<h1>Note Books</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @note_books.each do |note_book| %>
      <tr>
        <td><%= link_to 'Show', note_book %></td>
        <td><%= link_to 'Edit', edit_note_book_path(note_book) %></td>
        <td><%= link_to 'Destroy', note_book, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Note Book', new_note_book_path %>
